# GNU Make workspace makefile autogenerated by Premake

.NOTPARALLEL:

ifndef config
  config=release_native
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),release_native)
  fitLTC_config = release_native
endif
ifeq ($(config),release_x64)
  fitLTC_config = release_x64
endif
ifeq ($(config),release_x32)
  fitLTC_config = release_x32
endif
ifeq ($(config),debug_native)
  fitLTC_config = debug_native
endif
ifeq ($(config),debug_x64)
  fitLTC_config = debug_x64
endif
ifeq ($(config),debug_x32)
  fitLTC_config = debug_x32
endif

PROJECTS := fitLTC

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

fitLTC:
ifneq (,$(fitLTC_config))
	@echo "==== Building fitLTC ($(fitLTC_config)) ===="
	@${MAKE} --no-print-directory -C . -f fitLTC.make config=$(fitLTC_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f fitLTC.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  release_native"
	@echo "  release_x64"
	@echo "  release_x32"
	@echo "  debug_native"
	@echo "  debug_x64"
	@echo "  debug_x32"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   fitLTC"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"