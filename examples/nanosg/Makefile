# GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=release_x64
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),release_x64)
  viwewer_config = release_x64
endif
ifeq ($(config),release_x32)
  viwewer_config = release_x32
endif
ifeq ($(config),debug_x64)
  viwewer_config = debug_x64
endif
ifeq ($(config),debug_x32)
  viwewer_config = debug_x32
endif

PROJECTS := viwewer

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

viwewer:
ifneq (,$(viwewer_config))
	@echo "==== Building viwewer ($(viwewer_config)) ===="
	@${MAKE} --no-print-directory -C . -f viwewer.make config=$(viwewer_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f viwewer.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  release_x64"
	@echo "  release_x32"
	@echo "  debug_x64"
	@echo "  debug_x32"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   viwewer"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"